Template.logginform.events({

	'click .loggin':function(evt, tmpl){

	var email = tmpl.find('.mailid').value;
	var password = tmpl.find('.pword').value;
	Meteor.loginWithPassword(email, password,alert('Username or password incorrect!!'));
}
})

Template,logginform.logginerror = function(){

	alert("hello");

}
