
Router.map( function () {
  this.route('splash', {
    path: '/'      
  });
  
  this.route('home', {
    path: '/home'
  });

  this.route('navigation', {
    path: '/navigation'
  });

  this.route('sidebar', {
    path: '/sidebar'
  });

   this.route('post', {
    path: '/post'
  });

  this.route('event', {
    path: '/event'
  });

  this.route('duties', {
    path: '/duties'
  });
} )
